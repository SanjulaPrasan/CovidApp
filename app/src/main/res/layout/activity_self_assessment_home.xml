<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.SelfAssessmentHome"
    android:background="@color/white">

    <TextView
        android:id="@+id/tvSelfAss"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:text="Self Assessment"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textColor="#000000"
        android:textSize="40sp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RelativeLayout
        android:id="@+id/rlBackground"
        android:layout_width="match_parent"
        android:layout_height="550dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded_rectangle"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvSelfAss" />

    <ImageView
        android:id="@+id/imFever"
        android:src="@drawable/img_fever2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toTopOf="@+id/rlBackground"
        app:layout_constraintLeft_toLeftOf="@+id/rlBackground"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="96dp"
        android:layout_height="39dp"
        android:layout_marginLeft="30dp"
        android:text="Fever"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imFever"
        app:layout_constraintLeft_toRightOf="@id/imFever"
        app:layout_constraintTop_toTopOf="@+id/imFever" />

    <androidx.appcompat.widget.SwitchCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/switchBtnFever"
        android:textOff="No"
        android:textOn="Yes"
        android:thumb="@drawable/thumb"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:showText="true"
        app:track="@drawable/track"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ivDry"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/img_dry"
        app:layout_constraintTop_toBottomOf="@+id/imFever"
        app:layout_constraintLeft_toLeftOf="@+id/rlBackground"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="100dp"
        android:layout_height="32dp"
        android:layout_marginLeft="30dp"
        android:text="Dry Cough"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivDry"
        app:layout_constraintLeft_toRightOf="@id/ivDry"
        app:layout_constraintTop_toTopOf="@+id/ivDry" />

    <androidx.appcompat.widget.SwitchCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/switchBtnCough"
        android:textOff="No"
        android:textOn="Yes"
        android:thumb="@drawable/thumb"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        app:showText="true"
        app:track="@drawable/track"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/ivFati"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/img_fatigu"
        app:layout_constraintTop_toBottomOf="@+id/ivDry"
        app:layout_constraintLeft_toLeftOf="@+id/rlBackground"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="105dp"
        android:layout_height="31dp"
        android:layout_marginLeft="30dp"
        android:text="Fatigue"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivFati"
        app:layout_constraintLeft_toRightOf="@id/ivFati"
        app:layout_constraintTop_toTopOf="@+id/ivFati" />

    <androidx.appcompat.widget.SwitchCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/switchBtnFatigue"
        android:textOff="No"
        android:textOn="Yes"
        android:thumb="@drawable/thumb"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/textView5"
        app:showText="true"
        app:track="@drawable/track"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/ivbrea"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/img_breath"
        app:layout_constraintTop_toBottomOf="@+id/ivFati"
        app:layout_constraintLeft_toLeftOf="@+id/rlBackground"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="106dp"
        android:layout_height="55dp"
        android:layout_marginLeft="30dp"
        android:text="Breathing Difficulty"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivbrea"
        app:layout_constraintLeft_toRightOf="@id/ivbrea"
        app:layout_constraintTop_toTopOf="@+id/ivbrea" />

    <androidx.appcompat.widget.SwitchCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/switchBtnBreathing"
        android:textOff="No"
        android:textOn="Yes"
        android:thumb="@drawable/thumb"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/textView6"
        app:showText="true"
        app:track="@drawable/track"
        tools:ignore="MissingConstraints" />

    <RelativeLayout
        android:id="@+id/rlCovid"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:background="@drawable/rounded_rectangle1"
        app:layout_constraintTop_toBottomOf="@+id/ivbrea"
        tools:layout_editor_absoluteX="10dp" />
    <ImageView
        android:id="@+id/ivCovid"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/img_covid"
        app:layout_constraintTop_toTopOf="@+id/rlCovid"
        app:layout_constraintBottom_toBottomOf="@+id/rlCovid"
        app:layout_constraintLeft_toLeftOf="@+id/rlBackground"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="102dp"
        android:layout_height="51dp"
        android:layout_marginLeft="30dp"
        android:text="Covid-19 Positive"
        android:textSize="20sp"
        android:textColor="@color/reg_tv"
        app:layout_constraintBottom_toBottomOf="@+id/ivCovid"
        app:layout_constraintLeft_toRightOf="@id/ivCovid"
        app:layout_constraintTop_toTopOf="@+id/ivCovid" />

    <androidx.appcompat.widget.SwitchCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/switchBtnCovid"
        android:textOff="No"
        android:textOn="Yes"
        android:thumb="@drawable/thumb"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:showText="true"
        app:track="@drawable/track"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnSubmit"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/rounded_rectangle2"
        android:text="Submit"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/rlBackground"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rlCovid" />




    <Button
        android:id="@+id/btnHome"
        android:layout_width="320dp"
        android:layout_height="55dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="25dp"
        android:text="Home"
        android:textSize="20sp"
        android:textAlignment="center"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/rlBackground"
        android:layout_marginBottom="30dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@drawable/rounded_rectangle"
        />



</androidx.constraintlayout.widget.ConstraintLayout>